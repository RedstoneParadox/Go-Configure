{
  "schemaVersion": 1,
  "id": "goconfigure_test",
  "version": "${version}",

  "name": "Go Configure Test Mod",
  "description": "Test Mod for Go Configure.",
  "authors": [
    "RedstoneParadox"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/RedstoneParadox/paradox-config"
  },

  "license": "MIT",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "io.github.redstoneparadox.goconfigure.ParadoxConfigTest"
      }
    ]
  },
  "depends": {
    "fabricloader": ">=0.4.0",
    "fabric-language-kotlin": ">=1.3.71"
  },

  "custom": {
    "pconfig": {
      "package": "io.github.redstoneparadox.goconfigure",
      "configs": ["ExampleConfig"]
    }
  }
}
